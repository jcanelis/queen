{
  "expo": {
    "name": "TrackQueen",
    "slug": "tq22",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "Learn more about your music.",
    "icon": "./assets/icon.png",
    "scheme": "tq22",
    "userInterfaceStyle": "automatic",
    "platforms": ["android", "ios"],
    "githubUrl": "https://github.com/jcanelis/trackqueen",
    "privacy": "hidden",
    "owner": "jcanelis",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "129",
      "bundleIdentifier": "com.jcanelis.tq22",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio", "fetch"],
        "NSMicrophoneUsageDescription": "Enable the microphone to find songs using nearby audio."
      },
      "usesIcloudStorage": false,
      "usesAppleSignIn": false,
      "accessesContactNotes": false
    },
    "extra": {
      "eas": {
        "projectId": "4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
      }
    },
    "plugins": ["expo-build-properties"],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "android": {
      "package": "com.jcanelis.tq22"
    }
  }
}
