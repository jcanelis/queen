{
  "expo": {
    "name": "TrackQueen",
    "slug": "tq22",
    "version": "1.0.5",
    "orientation": "portrait",
    "description": "Learn more about your music.",
    "icon": "./assets/icon.png",
    "scheme": "tq22",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/jcanelis/trackqueen",
    "owner": "jcanelis",
    "splash": {
      "image": "./assets/tile.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "jsEngine": "hermes",
    "ios": {
      "jsEngine": "hermes"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.jcanelis.tq22",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio", "fetch"],
        "NSMicrophoneUsageDescription": "Enable the microphone to find songs using nearby audio."
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "usesIcloudStorage": false,
      "usesAppleSignIn": false,
      "accessesContactNotes": false
    },
    "extra": {
      "eas": {
        "projectId": "4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
      }
    },
    "plugins": ["expo-build-properties", "expo-secure-store", "expo-asset"],
    "runtimeVersion": "1.0.0"
  }
}
